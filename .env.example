# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration (Optional - for logging and memory features)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Memory Configuration
# Set to true to enable database logging via Supabase
ENABLE_DATABASE_LOGGING=true
# Set to true to enable file-based logging
ENABLE_FILE_LOGGING=false
# Default file path for logs (when file logging is enabled)
DEFAULT_LOG_FILE_PATH=./logs/execution.jsonl