{
  "name": "ninja-agents",
  "version": "1.0.0",
  "description": "Composable AI agent orchestration package (Shinobi, Kata, Shuriken, Orchestration, Utils)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "dev": "tsx internal/orchestrationDemo.ts",
    "demo": "tsx internal/orchestrationDemo.ts",
    "docs:generate": "typedoc",
    "docs:serve": "typedoc --watch",
    "docs:json": "typedoc --json docs/api.json --includeReferencedTypes"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "chalk": "^5.0.0",
    "dotenv": "^16.0.0",
    "openai": "^4.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "tsx": "^4.0.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.0.0"
  }
}